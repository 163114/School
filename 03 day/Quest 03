int main()
{
	int sum, diff, product, quotient;
  scanf("%d %d", first, second)

  sum = first + second;
	diff = first - second;
	product = first * second;
	quotient = first / second;
	
	printf("%d %d %d %d\n", sum, diff, product, quotient)

	return 0;
}
